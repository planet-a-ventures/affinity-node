{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "7.9.0",
        "generators": {
            "v2": {
                "generatorName": "typescript",
                "output": "src/v2/generated",
                "glob": "openapi/*.json",
                "additionalProperties": {
                    "supportsES6": true,
                    "platform": "deno",
                    "useObjectParameters": true,
                    "moduleName": "Affinity",
                    "projectName": "@planet-a/affinity-node/v2"
                }
            }
        }
    }
}
